<?xml version="1.0" encoding="utf-8"?>
<Project DefaultTargets="Build" ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup Label="UserMacros">
    <SolclientDir>$(SolutionDir)..\..</SolclientDir>
    <SolclientPostBuildCmd>xcopy /Y</SolclientPostBuildCmd>
    <SolclientMacros>SOLCLIENT_CONST_PROPERTIES</SolclientMacros>
  </PropertyGroup>
  <PropertyGroup>
    <_ProjectFileVersion>10.0.40219.1</_ProjectFileVersion>
  </PropertyGroup>
  <ItemGroup>
    <BuildMacro Include="SolclientDir">
      <Value>$(SolclientDir)</Value>
    </BuildMacro>
    <BuildMacro Include="SolclientPostBuildCmd">
      <Value>$(SolclientPostBuildCmd)</Value>
    </BuildMacro>
    <BuildMacro Include="SolclientMacros">
      <Value>$(SolclientMacros)</Value>
    </BuildMacro>
  </ItemGroup>
</Project>